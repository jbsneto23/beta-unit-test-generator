<?xml version="1.0" encoding="UTF-8"?>
<test-suite>
    <machine-name>Team</machine-name>
    <machine-invariant>
        <invariant-clause>card(team) = 11</invariant-clause>
        <invariant-clause>team &lt;: PLAYER</invariant-clause>
    </machine-invariant>
    <operation-under-test>substitute</operation-under-test>
    <testing-strategy>Input Space Partition</testing-strategy>
    <coverage-criteria>Equivalent Classes (All-Combinations)</coverage-criteria>
    <oracle-strategy>
        <state-variables>true</state-variables>
        <return-variables>true</return-variables>
        <invariant-ok>true</invariant-ok>
    </oracle-strategy>
    <test-cases>
        <test-case>
            <id>1</id>
            <formula>rr : PLAYER &amp; team &lt;: PLAYER &amp; not(rr /: team) &amp; card(team) = 11 &amp; not(pp : team)</formula>
            <preamble/>
            <isNegative>true</isNegative>
            <state-variables>
                <variable>
                    <identifier>team</identifier>
                    <values>
                        <value>PLAYER2</value>
                        <value>PLAYER3</value>
                        <value>PLAYER4</value>
                        <value>PLAYER5</value>
                        <value>PLAYER6</value>
                        <value>PLAYER7</value>
                        <value>PLAYER8</value>
                        <value>PLAYER9</value>
                        <value>PLAYER10</value>
                        <value>PLAYER11</value>
                        <value>PLAYER12</value>
                    </values>
                </variable>
            </state-variables>
            <operation-parameters>
                <parameter>
                    <identifier>pp</identifier>
                    <values>
                        <value>PLAYER1</value>
                    </values>
                </parameter>
                <parameter>
                    <identifier>rr</identifier>
                    <values>
                        <value>PLAYER2</value>
                    </values>
                </parameter>
            </operation-parameters>
        </test-case>
